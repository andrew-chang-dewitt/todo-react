{
  "name": "todo-app",
  "version": "0.0.1",
  "scripts": {
    "lint": "prettier --config .prettierrc.yaml --write './(*.html|*.js|*.json|src/**/*.ts|src/**/*.tsx)'",
    "test": "mocha",
    "coverage": "nyc npm run test",
    "prebuild": "npm run lint && npm run coverage",
    "build": "webpack",
    "dev": "webpack-dev-server --no-inline --open"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "text"
    ],
    "check-coverage": true,
    "lines": 99,
    "functions": 99,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/dist/**",
      "webpack.config.js",
      "**/*.spec.ts",
      "**/*.spec.tsx"
    ]
  },
  "mocha": {
    "extension": [
      "ts",
      "tsx"
    ],
    "spec": [
      "src/**/*.spec.tsx",
      "src/**/*.spec.ts"
    ],
    "require": "ts-node/register"
  },
  "author": "Andrew Chang-DeWitt",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "lodash": "^4.17.15",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
